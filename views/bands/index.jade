extends ../template/layout

block content
	.row
		.small-12.columns
			h1 Hello #{user.name}, this is #{band.name}'s Dahsboard
	.row
		.small-12.columns
			ul
				for mems in band.members
					li= mems.name
	.row
		.small-12.columns
			h2 View Your Calendar
			a(href='/calendar') Go
	.row
		.small-12.columns
			h2 Missing Some Members?
			a(href='/memberAdd') Add A Member
